rm -f index.bundle.js vendor.bundle.js index.html image
if [ "$1" = "--playground" ]; then
  cp -r ./playground/images ./playground/
  cp ./playground/index.bundle.js ./playground/vendor.bundle.js ./playground/index.html ./playground/
else
  cp -r ./dist/images ./dist/
  cp ./dist/index.bundle.js ./dist/vendor.bundle.js ./dist/index.html ./dist/
fi